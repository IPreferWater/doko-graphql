type GetPosts {
	posts:    [Post!]!
}

type Post {
    title: String!
    txt: String
    gps : Gps!
}

type Gps {
    x: Float!
    y: Float!
}

input InputPost {
    title: String!
    txt: String
    gps: InputGps!
}

input InputGps {
    x: Float!
    y: Float!
}

extend type Query {
  posts: GetPosts!
}

input InputLogin {
    username: String!
    password: String!
}

extend type Mutation {
 createPosts(input: [InputPost!]!): String!
 login(input: InputLogin!): String!
}